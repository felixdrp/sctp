{
  "name": "sctp",
  "version": "0.0.13",
  "author": {
    "name": "Vladimir Latyshev",
    "email": "latysheff@gmail.com"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "ip": "^1.1.5",
    "polycrc": "^0.1.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.16.0",
    "mocha": "^5.0.0",
    "raw-socket": "^1.5.2",
    "supports-color": "^5.1.0",
    "xo": "^0.18.2"
  },
  "description": "SCTP network protocol (RFC4960) in plain Javascript",
  "keywords": [
    "RFC4960",
    "SCTP",
    "Sigtran",
    "SS7",
    "RFC8261",
    "DTLS",
    "WebRTC"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "test": "xo && mocha"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "extends": ".eslintrc",
    "overrides": [
      {
        "files": "test/*.js",
        "rules": {
          "max-len": [
            "error",
            {
              "code": 300
            }
          ]
        }
      }
    ]
  },
  "repository": "https://github.com/latysheff/node-sctp.git"
}
